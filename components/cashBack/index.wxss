/**index.wxss**/
.cash{
  height: 100vh;
  background: rgba(0, 0, 0, 0.3);
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
}
.cash .wrap{
  background: #FFFFFF;
  border-radius: 32rpx 32rpx 0rpx 0rpx;    
  position: absolute;
  bottom: 0;
  width: 100%;
  overflow: hidden;
  padding: 0 28rpx;
  box-sizing: border-box;
}
.cash .cash-top{
  height: 122rpx;
  background: #FFFFFF;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #252525;
  line-height: 122rpx;
  text-align: center;
  position: relative; 
}
.cash .cash-top .close-wrap{
  position: absolute;
  top: 50%;
  right: 0;
  margin-top: -30rpx;
  width: 60rpx;
  height: 60rpx;
}
.cash .cash-top .close{
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  top: 50%;
  right: 0;
  margin-top: -13rpx;
}
.cash .cash-center .cash-item{
  border-bottom: 2rpx solid #D7D7D7;
  box-sizing: border-box;
  overflow: hidden;
}
.cash .cash-center .cash-item .cash-item-top{
  display: flex;
  align-items: center;
  margin-top: 32rpx;
}
.cash .cash-center .cash-item .cash-item-top .selected{
  width: 28rpx;
  height: 28rpx;
}
.cash .cash-center .cash-item .cash-item-top .selecte{
  width: 28rpx;
  height: 28rpx;
  border-radius: 50%;
  border: 2rpx solid #D6D6D6;
  box-sizing: border-box;
}
.cash .cash-center .cash-item .cash-item-top .desc{
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2F2F2F;
  margin: 0 10rpx 0 26rpx;
}
.cash .cash-center .cash-item .cash-item-bottom{
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(47, 47, 47, 0.6);
  margin: 20rpx 0 22rpx 54rpx;
}
.cash .cash-center .cash-item .remark {
  position: relative;
  margin-bottom: 22rpx;
}
.cash .cash-center .cash-item textarea{
  width: 100%;
  height: 184rpx;
  border-radius: 12rpx;
  border: 2rpx solid #979797;
  padding: 26rpx 0 0 10rpx;
  box-sizing: border-box;
}
.cash .cash-center .cash-item .remark .tip{
  position: absolute;
  bottom: 22rpx;
  right: 22rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(47, 47, 47, 0.4);
}
.cash .cash-center .cash-item .cash-item-top .price{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #D40000;
  padding: 0 10rpx;
  border-radius: 18rpx;
  border: 2rpx solid #D40000;
  box-sizing: border-box;
}
.cash .cash-center .cash-item .cash-item-top .free{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #3260EB;
  padding: 0 10rpx;
  border-radius: 18rpx;
  border: 2rpx solid #3260EB;
  box-sizing: border-box;
}
.cash .cash-bottom{
  display: flex;
  justify-content: space-between;
  margin-top: 74rpx;
  margin-bottom: 22rpx;
}
.cash .cash-bottom .cancle{
  width: 290rpx;
  height: 96rpx;
  border-radius: 54rpx;
  opacity: 0.9;
  border: 2rpx solid #466FED;
  box-sizing: border-box;
  line-height: 96rpx;
  text-align: center;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #466FED;
  }
  .cash .cash-bottom .sure{
    width: 290rpx;
    height: 96rpx;
    border-radius: 54rpx;
    opacity: 0.9;
    background: #3260EB;
    line-height: 96rpx;
    text-align: center;
    font-size: 32rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #FFFFFF;
    }