/**index.wxss**/
.insured{
 background-color: #F2F3F7;
 min-height: 100vh;
 padding: 20rpx 16rpx 102rpx;
 box-sizing: border-box;
}
.insured .insured-top{
  height: 252rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  padding: 26rpx 30rpx;
  box-sizing: border-box;

}
.insured .insured-top .insured-top-wrap{
  height: 44rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.insured .insured-top .insured-item-left{
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #676767;
}
.insured .insured-top .insured-item-left .star{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #D40000;
  margin-right: 4rpx;
}
.insured .insured-top .selected{
  width: 28rpx;
  height: 28rpx;
}
.insured .insured-center{
  margin-top: 18rpx;
}  
.insured .insured-center .name{
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #2F2F2F;
}
.insured .insured-bottom{
  border-bottom: 2rpx solid #D2D2D2;
  display: flex;
  justify-content: space-between;
  padding: 14rpx 0;
}
.insured .insured-bottom .insured-bottom-left{
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(47, 47, 47, 0.8);
}
.insured .insured-bottom .insured-bottom-right{
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #F38B00;
}
.insured .insured-content{
  background: #FFFFFF;
  border-radius: 16rpx;
  margin-top: 24rpx;
  padding: 32rpx 36rpx 0;
  overflow: hidden;
}
.insured .insured-content .btn{
  display: flex;
  justify-content: space-between;
  margin-top: 46rpx;
  margin-bottom: 72rpx;
}
.insured .insured-content .btn .cancle,.insured .insured-content .btn .sure{
  width: 290rpx;
  height: 96rpx;
  border-radius: 54rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 94rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
}
.insured .insured-content .btn .cancle{
  border: 2rpx solid #466FED;
  color: #466FED;
}
.insured .insured-content .btn .sure{
  background: rgba(186, 210, 255, 1);
  color: #FFFFFF;
} 
.insured .insured-content .btn .sure.active{
  background: #466FED;
} 