/**index.wxss**/
.goods{
  padding: 0 28rpx;
  margin-top: 30rpx;
}
.goods .item{
  border-bottom: 2rpx solid #D7D7D7;
  box-sizing: border-box;
  position: relative;
}
.goods .tips{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #D40000;
  margin-top: 4rpx;
}
.goods .goods-name .top{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2F2F2F;
}
.goods .goods-name .star{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #D40000;
  margin-right: 4rpx;
}
.goods .goods-name .top-right{
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #3260EB;
}
.goods .goods-name .bottom .name{
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #2F2F2F;
  margin: 20rpx 0 12rpx 0;
}
.goods .goods-name .bottom .type{
  display: inline-block;
  padding: 6rpx 18rpx 6rpx 28rpx;
  background: #EFEFEF;
  border-radius: 22rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(22, 22, 25, 0.6);
  margin: 20rpx 0 12rpx 0;
}
.goods .goods-name .bottom .close{
  width: 16rpx;
  height: 16rpx;
  margin-left: 18rpx;
}
.goods .error-active{
  border-bottom-color: #E50808;
}
.goods .item .error{
  position: absolute;
  bottom: -32rpx;
  left: 0;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #E50808;
  display: none;
}
.goods .error-active .error{
 display: block;
}
.goods .error-active .error.error-width{
  width: 192rpx;
}
.goods .goods-weight{
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.goods .goods-weight .star{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #D40000;
  margin-right: 4rpx;
}
.goods .goods-weight .left{
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2F2F2F;
}
.goods .goods-weight  .right,.goods .goods-volume .volume .volume-top .right{
  background: #EDEDED;
  border-radius: 12rpx;
  width: 274rpx;
  height: 60rpx;
  display: flex;
  border: 2rpx solid #E5E5E5;
  border-radius: 8rpx;
  justify-content: space-between;
  box-sizing: border-box;
  position: relative;
}
.goods .goods-weight  .slice,.goods .goods-weight  .add,.goods .goods-volume .slice,.goods .goods-volume .add{
  width: 80rpx;
  height: 100%;
  background: #EDEDED;
  display: flex;
  justify-content: center;
  align-items: center;
}
.goods .goods-weight  image,.goods .goods-volume image{
  width: 32rpx;
  height: 32rpx;
}
.goods .goods-weight  .right .weight-wrap{
  position: relative;
  display: flex;
}
.goods .goods-weight .weight,.goods .goods-volume .weight{
  width: 80rpx;
  height: 100%;
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC; 
  font-weight: 500;
  color: #2F2F2F;
  background-color: #fff;
  text-align: right;
  position: relative;
  box-sizing: border-box
}
.goods .goods-weight .unit,.goods .goods-volume .unit{
  width: 36rpx;
  height: 100%;
  font-size: 24rpx;
  color: #2F2F2F;
  background-color: #fff;
  font-weight: 400;
  line-height: 56rpx;
}
.goods .goods-volume .volume-wrap{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
}
.goods .goods-volume .volume-wraps{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 100%;
}
.goods .goods-volume .left{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #D40000;
}
.goods .goods-volume .right{
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(47, 47, 47, 0.6);
  display: flex;
  align-items: center;
}
.goods .goods-volume .right .switch{
  margin-left: 22rpx;
}
.goods .goods-volume .volume{
  height: 220rpx;
  background: #F2F2F2;
  border-radius: 12rpx;
  overflow: hidden;
}
.goods .goods-volume-active{
  border-bottom: none;
}
.goods .goods-volume .volume .volume-top{
  display: flex;
  justify-content: space-between;
  align-items: baseline;
  height: 60rpx;
  margin: 30rpx 30rpx 40rpx;
  position: relative;
  /* overflow: hidden; */
}
.goods .goods-volume .volume .volume-top .error{
  left: 140rpx;
}
.goods .goods-volume .volume .volume-top .left{
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2F2F2F;
  height: 60rpx;
}
.goods .goods-volume .volume .volume-top .right{
  width: 490rpx;
  border: none;
  background-color: #fff;
}
 .goods .goods-volume .slice, .goods .goods-volume .add{
   background: #FFFFFF;
   width: 126rpx;
 }
 .goods .goods-volume .slice{
   border-right: 2rpx solid #E6E6E6;
 }
 .goods .goods-volume .add{
  border-left: 2rpx solid #E6E6E6;
}
.goods .goods-volume .weight{
  text-align: right;
  width: 116rpx;
  box-sizing: border-box;
}
.goods .goods-volume .box{
  display: flex;
  justify-content: flex-start;
  width: 232rpx;
  position: relative;
}
 .goods .goods-volume .unita {
    padding-left: 0;
    position: absolute;
    top: 2rpx;
    right: 64rpx;
    color: rgba(47, 47, 47, 0.4);
 }
 .goods .goods-volume .volume-bottom{
   display: flex;
   justify-content: space-between;
   align-items: center;
   margin: 0 30rpx;
 }
 .goods .goods-volume .volume-bottom .item{
   width: 174rpx;
   height: 60rpx;
   background: #FFFFFF;
   border-radius: 4rpx;
   padding: 0rpx 8rpx 0rpx 26rpx;
   box-sizing: border-box;
   display: flex;
   align-items: center;
 }
 .goods .goods-volume .volume-bottom .standard{
    width: 76rpx;
    z-index: 999;
 }
 .goods .goods-volume .volume-bottom .item .units{ 
   height: 40rpx;
   line-height: 36rpx;
   border-left: 2rpx solid #E6E6E6;
   padding-left: 14rpx;
   font-size: 24rpx;
   font-family: PingFangSC-Regular, PingFang SC;
   font-weight: 400;
   color: rgba(47, 47, 47, 0.4);
   position: absolute;
   right: 8rpx;
 }
 .goods .goods-num .goods-num-top{
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
 }
 .goods .goods-num .goods-num-bottom{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(47, 47, 47, 0.4);
  margin-bottom: 40rpx;
 }
 .goods .goods-num .goods-num-bottom text{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #466FED;
 }
 .goods .goods-num{
  flex-direction: column;
  align-items: flex-start;
  height: auto;
 }
 .goods .goods-pack{
   display: flex;
   justify-content: space-between;
   height: 100rpx;
   align-items: center;
 }
 .goods .goods-pack .star{
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #D40000;
  margin-right: 4rpx;
 }
 .goods .goods-pack .top-left{
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2F2F2F;
 }
 .goods .goods-pack .right-arrow{
  width: 12rpx;
  height: 16rpx;
  margin-left: 16rpx;
 }
 .goods .goods-pack .item-right{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  max-width: 540rpx;
 }

 .goods .goods-pack .item-right .placeholders{
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #2F2F2F;
}
.goods .goods-picture .top-left{
  margin-top: 32rpx;
  margin-bottom: 30rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2F2F2F;
}
.goods .goods-picture .goods-picture-wrap{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.goods .goods-picture .goods-picture-wrap .picture-item,.goods .goods-picture .goods-picture-wrap .picture-item-list{
  width: 206rpx;
  height: 206rpx;
  background: #F5F5F5;
  border-radius: 8rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-bottom: 24rpx;
}
.goods .goods-picture .goods-picture-wrap .picture-item image{
  width: 70rpx;
  height: 64rpx;
}
.goods .goods-picture .goods-picture-wrap .picture-item text{
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(47, 47, 47, 0.6);
  margin-top: 24rpx;
}
.goods .goods-picture .goods-picture-wrap .picture-item-list .imgs{
  width: 100%;
  height: 100%;
}
.goods .goods-picture .goods-picture-wrap .picture-item-list{
  margin-right: 38rpx;
  position: relative;
}
.goods .goods-picture .goods-picture-wrap .picture-item-list .del{
  position: absolute;
  top:0;
  right: 0;
  width: 40rpx;
  height: 40rpx;
}
.goods .goods-picture .goods-picture-wrap .picture-item-list:nth-of-type(3n){
  margin-right: 0;
}
.goods .remark {
  position: relative;
}
.goods .remark textarea{
  width: 100%;
  height: 184rpx;
  border-radius: 12rpx;
  border: 2rpx solid #979797;
  padding: 26rpx 0 0 10rpx;
  box-sizing: border-box;
}
.goods .remark .tip{
  position: absolute;
  bottom: 22rpx;
  right: 22rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(47, 47, 47, 0.4);
}
.goods .goods-bottom{
  width: 100%;
  height: 120rpx;
  background: #FFF;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2;
  padding-top: 20rpx;
}
.goods .goods-bottom .btn{
  width: 638rpx;
  height: 96rpx;
  background: rgba(186, 210, 255, 1);
  border-radius: 54rpx;
  opacity: 0.9;
  text-align: center;
  line-height: 96rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  margin: 0 auto;
}
.goods .goods-bottom .btn.active{
  background: rgba(50, 96, 235, 1);
}