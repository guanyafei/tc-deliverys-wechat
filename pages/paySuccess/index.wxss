/**index.wxss**/
.pay-success{
  background: #FFFFFF;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.pay-success .img{
  width: 192rpx;
  height: 168rpx;
  margin-top: 200rpx;
}
.pay-success .tip{
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2F2F2F;
  margin-top: 24rpx;
}
.pay-success .courier{
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(47, 47, 47, 0.6);
  margin-top: 48rpx;
}
.pay-success .num-wrap{
  font-size: 46rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2F2F2F;
  margin-top: 14rpx;
}
.pay-success .btn{
  margin: 0 auto;
  font-size: 31rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  width: 638rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  background: rgb(8, 23, 227);
  border-radius: 54rpx;
  margin-top: 360rpx;
}