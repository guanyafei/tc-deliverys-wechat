/**index.wxss**/
.home{
  padding: 28rpx;
  background: #F2F3F7;
  height: calc(100vh - 56rpx);
}
.banner{
  width: 694rpx;
  height: 300rpx; 
}
.box{
  width: 100%;
  height: 552rpx;
  background: #FFFFFF;
  border-radius: 6px;
  margin-top: 24rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.item{
  width: 347rpx;
  height: 184rpx;
  box-sizing: border-box;
  flex-shrink: 0;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  
}
.item:nth-of-type(1),.item:nth-of-type(3){
  border-bottom: 2rpx solid #DDDDDD;
  border-right: 2rpx solid #DDDDDD;
}
.item:nth-of-type(2),.item:nth-of-type(4){
  border-bottom: 2rpx solid #DDDDDD;
}
.item:nth-of-type(5){
  border-right: 2rpx solid #DDDDDD;
}
.item .logo{
  width: 48rpx;
  height: 48rpx;
  margin: 0 24rpx 0 46rpx;
}
.item .nav{
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2F2F2F;
}